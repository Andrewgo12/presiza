{
    "name": "laravel-reportes",
    "version": "1.0.0",
    "description": "Sistema de Gestión de Evidencias - Laravel",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "watch": "vite build --watch",
        "hot": "vite --host",
        "production": "vite build --mode production",
        "lint": "eslint resources/js --ext .js,.vue",
        "lint:fix": "eslint resources/js --ext .js,.vue --fix",
        "format": "prettier --write resources/js/**/*.{js,vue}",
        "clean": "rm -rf public/build node_modules/.vite",
        "analyze": "vite-bundle-analyzer"
    },
    "devDependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "autoprefixer": "^10.4.16",
        "axios": "^1.6.1",
        "eslint": "^8.54.0",
        "laravel-vite-plugin": "^0.8.1",
        "postcss": "^8.4.31",
        "prettier": "^3.1.0",
        "tailwindcss": "^3.3.5",
        "vite": "^4.5.1",
        "vite-bundle-analyzer": "^0.7.0"
    },
    "dependencies": {
        "alpinejs": "^3.13.3",
        "chart.js": "^4.4.0",
        "chartjs-adapter-date-fns": "^3.0.0",
        "date-fns": "^2.30.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "keywords": [
        "laravel",
        "php",
        "evidence-management",
        "file-management",
        "tailwindcss",
        "alpinejs"
    ],
    "author": "Sistema de Gestión de Evidencias",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/company/laravel-reportes.git"
    },
    "bugs": {
        "url": "https://github.com/company/laravel-reportes/issues"
    },
    "homepage": "https://github.com/company/laravel-reportes#readme"
}
